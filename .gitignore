# .gitignore for Android Studio

# Ignore Gradle files
.gradle/
build/

# Ignore Android Studio specific files
.idea/
*.iml

# Ignore local configuration files
local.properties

# Ignore user-specific files
*.log
*.swp
*~

# Ignore compiled classes
*.class

# Ignore APK and other generated files
*.apk
*.ap_ 

# Ignore native build files
obj/
*.o
*.so

# Ignore Android Studio specific caches
*.hprof

# Ignore dependency cache
.gradle/

# Ignore external native build folder
.externalNativeBuild/

# Ignore Android packaging files
bin/
out/

# Ignore other build directories
build/

# Ignore lint result files
lint/intermediates/
